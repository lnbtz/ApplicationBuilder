# Copy this file to .env and adjust values for local dev.
# DO NOT commit your real .env.

# Postgres settings (used both by db and backend containers)
POSTGRES_DB=appdb
POSTGRES_USER=appuser
POSTGRES_PASSWORD=changeMe

# (Optional) OpenAI API key for backend features (leave blank if not needed yet)
OPENAI_API_KEY=

# If you prefer to explicitly set DB_URL instead of the inline expression in compose
# uncomment the next line. Otherwise the backend service uses jdbc:postgresql://db:5432/${POSTGRES_DB}
# DB_URL=jdbc:postgresql://db:5432/appdb
